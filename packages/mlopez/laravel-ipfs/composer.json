{
    "name": "mlopez/laravel-ipfs",
    "description": "Laravel package to communicate with IPFS",
    "keywords": [
        "mlopez",
        "laravel",
        "laravel-ipfs",
        "ipfs",
        "php"
    ],
    "homepage": "https://github.com/mlopez/laravel-ipfs",
    "license": "MIT",
    "authors": [
        {
            "name": "Manuel Lopez",
            "email": "malopezr7@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "league/flysystem": "^1.1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Mlopez\\IPFS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mlopez\\IPFS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mlopez\\IPFS\\IPFSServiceProvider"
            ],
            "aliases": {
                "IPFS": "Mlopez\\IPFS\\IPFSFacade"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
